<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Tugma foni -->
    <solid android:color="#4CAF50" /> <!-- Tugma foni rangi (yashil) -->

    <!-- Tugma chekkalari uchun radius -->
    <corners android:radius="16dp" /> <!-- Chekkalarni yumaloq qilish -->

    <!-- Tugma chekkalari -->
    <stroke
        android:width="2dp"
        android:color="#388E3C" /> <!-- Tugma chegarasi rangi va qalinligi -->

    <!-- Tugma ichki chet bo'sh joylari -->
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp" />

    <!-- Tugma o'lchami -->
    <size
        android:width="200dp"
        android:height="60dp" /> <!-- Tugma kattaligi (ixtiyoriy) -->
</shape>
